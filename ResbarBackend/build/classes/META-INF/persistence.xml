<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="ResbarBackendPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>sv.edu.uesocc.disenio2018.resbar.backend.Parametro</class>
    <class>sv.edu.uesocc.disenio2018.resbar.backend.Categoria</class>
    <class>sv.edu.uesocc.disenio2018.resbar.backend.Orden</class>
    <class>sv.edu.uesocc.disenio2018.resbar.backend.Producto</class>
    <class>sv.edu.uesocc.disenio2018.resbar.backend.DetalleOrden</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:mariadb://localhost:3306/"/>
      <property name="javax.persistence.jdbc.user" value="resbar"/>
      <property name="javax.persistence.jdbc.driver" value="org.mariadb.jdbc.Driver"/>
      <property name="javax.persistence.jdbc.password" value="Restaurante2018"/>
    </properties>
  </persistence-unit>
</persistence>
